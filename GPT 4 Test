nai import OpenAI\n","\n","def generate_best_answer(question, choices):\n","    client = OpenAI(api_key=\"\")\n","    choices_text = ' '.join([f\"Option {chr(65+i)}: {choice}\" for i, choice in enumerate(choices)])\n","    prompt = f\"Given the question: '{question}', and the choices: {choices_text}, select the best choice\"\n","\n","\n","    try:\n","        chat_completion = client.chat.completions.create(\n","            messages=[\n","                {\n","                    \"role\": \"user\",\n","                    \"content\": prompt,\n","                }\n","            ],\n","            model=\"gpt-4-turbo-preview\",  # Adjust the model as needed\n","        )\n","\n","        # Extract the content of the response\n","        facts = chat_completion.choices[0].message.content\n","        return facts.strip()\n","    except Exception as e:\n","        print(f\"Error generating facts: {e}\")\n","        return \"No facts generated due to an error.\"\n"],"metadata":{"id":"T66E-YBjFehC"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["from tqdm import tqdm"],"metadata":{"id":"HgnQI2h1H552"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# Initialize counters\n","correct_predictions = 0\n","total_questions = len(data[\"validation\"])\n","\n","# Define your OpenAI API key more securely\n","api_key = \"your_openai_api_key\"  # Consider using an environment variable or secure storage for this\n","OpenAI.api_key = api_key\n","\n","for item in tqdm(data[\"validation\"], desc=\"Processing Questions\"):\n","    question_id = item['id']\n","    question_stem = item['question_stem']\n","    answer_key = item['answerKey']\n","    choices_text = item['choices']['text']\n","\n","    # Use the new function to generate the best answer\n","    generated_answer = generate_best_answer(question_stem, choices_text)\n","\n","    # Determine the correct answer's index\n","    actual_answer_index = ord(answer_key) - ord('A')\n","\n","    #